{
  "manifest_version": 2,
  "name": "Buildkite Helper",
  "version": "0.1",
  "permissions": [
      "notifications",
      "tabs",
      "contextMenus",
      "https://buildkite.com/"
  ],
  "background": {
      "scripts": [
          "background.js",
          "jquery.min.js"
      ]
  },
  "browser_action": {
    "default_title": "Track this build."
  }
}
